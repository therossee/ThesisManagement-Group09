{
  "name": "back-end",
  "version": "1.0.0",
  "description": "The back end service for the Thesis Management System",
  "main": "index.js",
  "scripts": {
    "test": "jest --detectOpenHandles",
    "test:coverage": "jest --coverage --colors",
    "test:unit": "npx jest --coverage --coverageDirectory=coverage/coverage_unit --colors --runInBand users.unit.test.js thesis.unit.test.js database.test.js",
    "test:integration": "npx jest --coverage --coverageDirectory=coverage/coverage_integration  --runInBand --colors auth.integration.test.js users.integration.test.js thesis.integration.test.js",
    "start": "node index",
    "dev": "nodemon index"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "better-sqlite3": "^9.0.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "express": "^4.18.2",
    "express-session": "1.17.3",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "supertest": "^6.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1"
  },
  "jest": {
    "collectCoverage": true
  }
